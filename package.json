{
  "name": "where-to-go-web",
  "description": "Where To Go Web Application",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "local": "vite --mode developer_local --port 8080 --host",
    "dev": "vite --mode development --port 8080 --host",
    "build:local": "tsc && vite build --mode developer_local && rm -fv tsconfig*.tsbuildinfo",
    "build:dev": "tsc && vite build --mode development && rm -fv tsconfig*.tsbuildinfo",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "npx eslint .",
    "format": "npx prettier . --write"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "antd": "5.21.6",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@types/node": "22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "4.3.3",
    "eslint": "^9.14.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "prettier": "3.3.3",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.10"
  }
}
